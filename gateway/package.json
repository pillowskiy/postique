{
  "name": "gateway",
  "version": "1.0.0",
  "main": "./src/index.js",
  "type": "module",
  "imports": {
    "#shared/*": "./src/shared/*",
    "#api/*": "./src/api/*",
    "#app/*": "./src/app/*",
    "#lib/*": "./src/lib/*"
  },
  "scripts": {
    "dev:prepare": "npm run types && npm run build",
    "dev:start": "node src/index.js",
    "dev": "nodemon --config nodemon.local.json",
    "dev:deploy": "nodemon --config nodemon.deploy.json",
    "build": "gulp",
    "start": "node src/index.js",
    "purgecss": "node scripts/purgecss.js --content './views/**/*.ejs' --css './public/**/*.css' --output ./public/styles/postique.min.css",
    "types": "npm run types:views",
    "types:views": "node scripts/safeviews/index.js --dir views --output src/views.d.ts --engine ejs",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix",
    "lint:ejs": "./node_modules/.bin/ejslint views/",
    "format": "prettier --write .",
    "tidy": "depcheck",
    "postinstall": "patch-package"
  },
  "author": "pillowskiy",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.4",
    "@types/pino": "^7.0.4",
    "depcheck": "^1.4.7",
    "ejs-lint": "^2.0.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "gulp": "^5.0.0",
    "gulp-autoprefixer": "^9.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-changed": "^5.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^6.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "minimist": "^1.2.8",
    "nodemon": "^3.1.9",
    "patch-package": "^8.0.0",
    "prettier": "^3.5.1",
    "prettier-plugin-ejs": "^1.0.3",
    "purgecss": "^7.0.2",
    "rollup": "^4.38.0",
    "rollup-plugin-cjs-es": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.85.0",
    "tree-sitter": "^0.21.1",
    "tree-sitter-javascript": "^0.23.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
    "@postique/pb": "file:../contracts/gen/js",
    "@shoelace-style/shoelace": "^2.20.1",
    "awilix": "^12.0.4",
    "bootstrap": "^5.3.3",
    "compression": "^1.8.0",
    "cookie": "^1.0.2",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "gulp-purgecss": "^7.0.2",
    "helmet": "^8.0.0",
    "htmx.org": "1.9.12",
    "ky": "^1.8.1",
    "multer": "^1.4.5-lts.2",
    "nanoid": "^5.1.5",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "quill": "^2.0.3"
  },
  "patchedDependencies": {
    "htmx.org@1.9.12": "patches/htmx.org@1.9.12.patch"
  }
}
